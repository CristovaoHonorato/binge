{
  "name": "binge",
  "version": "3.0.2",
  "description": "Tool for managing monorepos with local packages",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run clean && babel src -d lib",
    "clean": "rimraf lib",
    "dev": "npm run clean && babel -w src -d lib",
    "lint": "eslint '{bin/**/*.js,src/**/*.js,test/**/*.js}'",
    "test": "mocha",
    "prepublish": "npm run lint && npm run prettier:check && npm run build",
    "prettier": "prettier --write --tab-width 4 --no-semi --single-quote --trailing-comma es5 '{bin/**/*.js,src/**/*.js,test/**/*.js}'",
    "prettier:check": "prettier --list-different --tab-width 4 --no-semi --single-quote --trailing-comma es5 '{bin/**/*.js,src/**/*.js,test/**/*.js}'"
  },
  "author": "Cristovao Honorato",
  "repository": "https://github.com/CristovaoHonorato/binge",
  "license": "ISC",
  "dependencies": {
    "async": "2.5.0",
    "chalk": "2.0.1",
    "chokidar": "1.7.0",
    "cross-spawn": "5.1.0",
    "fs-extra": "4.0.0",
    "invariant": "2.2.2",
    "meow": "3.7.0",
    "pad": "1.1.0",
    "semver": "5.3.0",
    "semver-intersect": "1.1.2",
    "signal-exit": "3.0.2"
  },
  "bin": {
    "binge": "./bin/binge.js"
  },
  "devDependencies": {
    "babel-cli": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "2.0.0-alpha.17",
    "chai": "4.1.0",
    "eslint": "4.2.0",
    "eslint-config-prettier": "2.3.0",
    "eslint-config-standard": "10.2.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "mocha": "3.4.2",
    "prettier": "1.5.3",
    "rimraf": "2.6.1"
  }
}
